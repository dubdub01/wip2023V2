{% extends "base.html.twig" %}

{% block body %}


{% if results is not empty %}
    <h2>Résultats de la recherche</h2>
    <ul>
        {% for result in results %}
            <li>{{ result }}</li>
        {% endfor %}
    </ul>
{% else %}
    <p>Aucun résultat trouvé.</p>
{% endif %}

{% endblock %}